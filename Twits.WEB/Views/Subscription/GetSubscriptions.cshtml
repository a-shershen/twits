@model IEnumerable<Twits.WEB.Models.SubscriptionUser>

@if (Model == null)
{
    <div class="col-xs-12">
        Нет подписок
    </div>
}

else
{
    if (Model.Count() == 0)
    {
        <div class="col-xs-12">
            Нет подписок
        </div>
    }

    else
    {
        foreach (var str in Model)
        {
            <div class="col-xs-12">
                @Html.ActionLink(@str, "UserInfo", "User", new { user = @str }, null)
            </div>

        }
    }
}
