@model IEnumerable<string>

@if (Model == null)
{

    @:Никто не подписан
}

else
{
    if (Model.Count() == 0)
    {
        @:Никто не подписан
    }

    else
    {
        foreach (var str in Model)
        {
            @Html.ActionLink(@str, "UserInfo", "User", new { user = @str }, null)
            <br />
        }
    }
}

