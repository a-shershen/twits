@model IEnumerable<Twits.WEB.Models.UserMessage>

@if (Model == null)
{
    <div class="col-xs-12">
        Нет сообщений
    </div>
}
else
{
    if (Model.Count() == 0)
    {
        <div class="col-xs-12">
            Нет сообщений
        </div>
    }

    else
    {
        foreach (var mes in Model)
        {
            <div class="container col-xs-12 messageDiv">
                @MessageHelpers.PrintMessage(mes)
            </div>
        }
    }
}
